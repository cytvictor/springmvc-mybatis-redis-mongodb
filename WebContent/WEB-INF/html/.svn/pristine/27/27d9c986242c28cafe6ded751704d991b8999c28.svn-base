<!doctype html>
<html>
<head>
	<!--#include virtual="../common/meta.shtml"-->
</head>
<body>
	<!--#include virtual="../common/header.shtml"-->
	<div class="container">
		<!--#include virtual="../common/side.shtml"-->
		<div class="content">
			<h4 class="contentNav messageManageNav">
				<a class="backup" href="messageManage.shtml">&lt;&lt;返回</a>
				发布消息
				<span class="navTab">
					<a href="javascript:;" class="currTab" index="0">我收到的消息</a>
					<a href="javascript:;" index="1">我发布的消息</a>
				</span>
			</h4>
			<ul class="detailBox noUnderline mt20">
				<li>
					<label class="cName"><b class="red"></b>消息名称：</label>
					<input type="text" class="msgTitle"/> 
				</li>
				<li>
					<label class="cName">消息类型：</label>
					<label><input type="radio" name="msgStyle">新闻</label>
					<label><input type="radio" name="msgStyle" class="ml20">活动</label>
					<label><input type="radio" name="msgStyle" class="ml20">公告</label>
				</li>
				<li>
					<label class="cName">消息内容：</label>
					<textarea class="cCont msgContent"></textarea>
				</li>
				<li>
					<label class="cName">发布时间：</label>
					<div class="cCont sendtimeSelect">
						<p><label><input type="radio">立即发布</label></p>
						<p>
							<label><input type="radio">定时发布</label>
							<select name="" id="">
								<option value="">请选择</option>
							</select>
						</p>
					</div>
				</li>
			</ul>
			<div class="submitBtn">
				<a class="btn">提交</a>
			</div>
		</div>
		
	</div>
	<!--#include virtual="../common/footer.shtml"-->
</body>
</html>