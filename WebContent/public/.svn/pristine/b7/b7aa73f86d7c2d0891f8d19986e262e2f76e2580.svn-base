/*图片滚动*/
#photo-scroll{width: 930px;overflow: hidden;zoom:1;position: relative;margin-bottom: 10px;}
#rollBox{margin: 0 auto;width: 864px;overflow: hidden;_position:relative;}
#rollList{float:left;overflow: hidden;zoom:1;margin-right: -30000px;}
#rollList li{float: left;padding: 0 5px;_display:inline;/* border: 3px solid transparent; */}
#rollList li img{border: 3px solid transparent;}
#rollList li.selected img{border: 3px solid #4EC0E5;}
#rollList li a{position:relative;}
#rollList img{display: block;}
.rollMenu{position: absolute;}
#rollLeft{left:0 ;top: 50%;margin-top: -18px;}
#rollRight{right:0; top: 50%; margin-top: -18px;}

/*图片滚动加tips*/
#rollList a{position: relative;display: block;overflow: hidden;*position:static;}
#rollList span{width:200px;height:28px;line-height: 28px;position: absolute;bottom: 0;left: 0;color:#fff;background-color: rgba(0,0,0,0.5); _background:#000;_filter:alpha(opacity=50);zoom:1;}

