.container2-secondary{width: 290px;float: right;overflow: hidden;}
.container2-secondary h4{display:inline-block;height:28px;overflow:hidden;padding: 0 10px;text-align:center;margin-bottom:20px; font-size: 16px;color:#fff;background-color: #4ec0e5;font-weight: normal;}
