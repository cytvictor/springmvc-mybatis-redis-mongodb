/*辅导课程--汉语言文学*/
.secNav h5{color: #666;font-size: 14px;height: 20px;line-height: 20px;margin-bottom: 20px;}
.fudaoBook{height: 470px;}
.fudaoBook img{float: left;}
.classDetail{overflow: hidden;zoom: 1;padding: 10px 40px;position: relative;}
.classDetail h4{font-size: 16px;width:580px;}
.classDetail ul li{line-height: 40px;font-size: 14px;}
.classDetail .btn{margin-top: 30px;}
.startOrNot span{width:147px;height:54px;position: absolute;right: 0;top: 0;background: url(../image/notStart.png) no-repeat 0 0;_background-image: url(../image/notStart.gif); text-align: center;line-height: 54px;font-size: 18px;padding-left: 10px;}
.clsPrimary{width: 700px;float: left;margin-right: 20px;display: inline;}
.clsSecondary{width: 447px;overflow: hidden;zoom:1;}
.clsBox{margin-top: 30px;}
.clsPrimary table{width: 700px;}
.clsPrimary table th,
.clsPrimary table td
{border: 1px solid #ccc;text-align: center;padding: 10px;}
.yhpl label{display: inline-block;vertical-align: top;}
.comment{width: 600px;height: 160px;background: #eee;display: inline-block;}
.comment textarea{width: 580px;height: 105px;margin: 10px;resize: none;font-size: 14px;}
.comment .face{background: url(../image/face.png) no-repeat 0 50%;padding-left: 25px;margin-left:20px;height: 25px;line-height:25px;display: inline-block;cursor: pointer;}
.comment .btn{float: right;margin-right: 30px;}
.leaveMsg{margin-top: 40px;font-size: 14px;}
.leaveMsg .user{width: 60px;height: 60px;float: left;}
.leaveMsg .msg{overflow: hidden;zoom: 1;padding-left: 20px;}
.leaveMsg .msg span{color: #888;}
.leaveMsg .msgList{margin-top: 20px;}

.clsSecondary{font-size: 14px;}
.clsSecondary h5{height: 50px;line-height: 50px;font-size: 16px;}

.detailBox{padding: 10px 0;border-bottom: 1px solid #ccc;}
.detailBox img{float: left;width: 100px;text-align: right;padding-right: 25px;vertical-align: middle;}
.detailMsg{overflow: hidden;zoom: 1;padding-left: 20px;font-size: 14px;color:#868686}
.detailMsg h5{height: 30px;line-height: 30px;color:#444}
.detailMsg h5 a{color:#444;font-weight: normal;}
.detailMsg a{color:#868686}


